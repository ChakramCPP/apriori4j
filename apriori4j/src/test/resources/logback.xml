<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="default" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date [%level] %msg%n</pattern>
            <charset>utf-8</charset>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="default"/>
    </root>
</configuration>
